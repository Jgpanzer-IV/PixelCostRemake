@model List<ListItem> 

@for (int i = 0; i < (@Model?.Count() ?? 0); i++)
{
    <div class="ExpenseList">
        <div class="d-flex justify-content-between">
            <div>
                <span class="LabelList">@(Model?.ElementAt(i).Name ?? "Undefinded")</span>
                <span class="PlaceHolder">@(Model?.ElementAt(i).Info ?? "")</span>
            </div>
            <div>
                <span class="ValueList text-end"><B>@(Model?.ElementAt(i).ValueElement ?? "Undefinded")</B></span>
                <span class="PlaceHolder">@(Model?.ElementAt(i).Description ?? "")</span>
            </div>
        </div>
    </div>
}