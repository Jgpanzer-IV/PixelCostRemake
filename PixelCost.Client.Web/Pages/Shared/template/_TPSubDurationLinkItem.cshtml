@model IEnumerable<ProgressItem> 

@for (int i = 0; i < (Model?.Count() ?? 0); i++)
{
    <a class="LinkItem" asp-area="Manager" asp-page="SubDurationManager" asp-route-durationID="TestDuration" asp-route-id="@Model?.ElementAt(i).ItemName">
        <partial name="asset/_ProgressItem" model="Model?.ElementAt(i)"></partial>
    </a>
}